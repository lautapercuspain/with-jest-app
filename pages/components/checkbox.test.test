```jsx
// Test 1: Check if the checkbox is rendered correctly
import React from 'react';
import { render } from '@testing-library/react';
import Checkbox from './Checkbox';

test('renders checkbox correctly', () => {
  const { getByLabelText } = render(<Checkbox checked={false} onChange={() => {}} label="Checkbox Label" />);
  const checkbox = getByLabelText('Checkbox Label');

  expect(checkbox).toBeInTheDocument();
  expect(checkbox.checked).toBe(false);
});

// Test 2: Check if the onChange event is triggered correctly
test('triggers onChange event correctly', () => {
  const handleChange = jest.fn();
  const { getByLabelText } = render(<Checkbox checked={false} onChange={handleChange} label="Checkbox Label" />);
  const checkbox = getByLabelText('Checkbox Label');

  fireEvent.click(checkbox);

  expect(handleChange).toHaveBeenCalledTimes(1);
  expect(handleChange).toHaveBeenCalledWith(true);
});
```

Note: Make sure to install `@testing-library/react` package and import `fireEvent` from the `@testing-library/react` in order to run these tests.